<?php
/**
 * @file
 * Code for the sr16splash Site Media Wall feature.
 */
include_once 'sr16splash_site_media_wall.features.inc';
include_once 'sr16splash_site_media_wall.custom.inc';

/**
 * Implements hook_ctools_plugin_directory().
 */
function sr16splash_site_media_wall_ctools_plugin_directory($module, $plugin) {
  if ($module == 'panels' && ($plugin == 'styles' || $plugin == 'layouts')) {
    return 'plugins/' . $plugin;
  }
}